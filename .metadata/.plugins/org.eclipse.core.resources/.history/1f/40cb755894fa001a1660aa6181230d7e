
public class Vehicle {
	
	private int wheels;
	private String fuel;
	
	Vehicle(int wheels, String fuel) {
		this.wheels = wheels;
		this.fuel = fuel;
	}

	public static void main(String[] args) {
		Vehicle aVehicle = new Vehicle(4,"gasoline");
		String s = aVehicle.toString();
		System.out.println(s);
	}

}